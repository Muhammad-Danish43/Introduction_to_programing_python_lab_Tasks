   "source": [
    "import math\n",
    "\n",
    "R = 50\n",
    "L = 0.1\n",
    "C = 100e-6\n",
    "f = 60\n",
    "\n",
    "w = 2 * math.pi * f\n",
    "\n",
    "Z_R = complex(R, 0)\n",
    "Z_L = complex(0, w * L)\n",
    "Z_C = complex(0, -1 / (w * C))\n",
    "\n",
    "print(\"Resistor Impedance:\", Z_R)\n",
    "print(\"Inductor Impedance:\", Z_L)\n",
    "print(\"Capacitor Impedance:\", Z_C)"
   ]
  }
