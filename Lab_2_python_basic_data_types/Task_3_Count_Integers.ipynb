    "import csv\n",
    "\n",
    "with open(\"integers.csv\", \"r\") as file:\n",
    "    reader = csv.reader(file)\n",
    "    numbers = [float(row[0]) for row in reader]\n",
    "\n",
    "count = sum(num.is_integer() for num in numbers)\n",
    "\n",
    "print(\"Total integers:\", count)"
   ]
  }
